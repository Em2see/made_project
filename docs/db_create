CREATE TABLE "points" (
	"id" VARCHAR NULL DEFAULT NULL,
	"x" INTEGER NULL DEFAULT NULL,
	"y" INTEGER NULL DEFAULT NULL,
	"height" DOUBLE PRECISION NULL DEFAULT NULL,
	"cap" DOUBLE PRECISION NULL DEFAULT NULL,
	"tech" INTEGER NULL DEFAULT NULL,
	"azimuth" DOUBLE PRECISION NULL DEFAULT NULL
)
;

CREATE TABLE "predict_arma" (
	"id" INTEGER NULL DEFAULT NULL,
	"date_" DATE NULL DEFAULT NULL,
	"x" DOUBLE PRECISION NULL DEFAULT NULL,
	"y" DOUBLE PRECISION NULL DEFAULT NULL,
	"spd_pred" DOUBLE PRECISION NULL DEFAULT NULL,
	"tech" INTEGER NULL DEFAULT NULL,
	"cap" DOUBLE PRECISION NULL DEFAULT NULL,
	"height" DOUBLE PRECISION NULL DEFAULT NULL,
	"azimuth" DOUBLE PRECISION NULL DEFAULT NULL
)
;

CREATE TABLE "predict_boosting" (
	"id" INTEGER NULL DEFAULT NULL,
	"date_" DATE NULL DEFAULT NULL,
	"x" DOUBLE PRECISION NULL DEFAULT NULL,
	"y" DOUBLE PRECISION NULL DEFAULT NULL,
	"spd_pred" DOUBLE PRECISION NULL DEFAULT NULL,
	"tech" INTEGER NULL DEFAULT NULL,
	"cap" DOUBLE PRECISION NULL DEFAULT NULL,
	"height" DOUBLE PRECISION NULL DEFAULT NULL,
	"azimuth" DOUBLE PRECISION NULL DEFAULT NULL
)
;

CREATE TABLE "predict_linear" (
	"id" INTEGER NULL DEFAULT NULL,
	"date_" DATE NULL DEFAULT NULL,
	"x" DOUBLE PRECISION NULL DEFAULT NULL,
	"y" DOUBLE PRECISION NULL DEFAULT NULL,
	"spd_pred" DOUBLE PRECISION NULL DEFAULT NULL,
	"tech" INTEGER NULL DEFAULT NULL,
	"cap" DOUBLE PRECISION NULL DEFAULT NULL,
	"height" DOUBLE PRECISION NULL DEFAULT NULL,
	"azimuth" DOUBLE PRECISION NULL DEFAULT NULL
)
;

CREATE TABLE "proove" (
	"id" INTEGER NULL DEFAULT NULL,
	"date_" DATE NULL DEFAULT NULL,
	"x" DOUBLE PRECISION NULL DEFAULT NULL,
	"y" DOUBLE PRECISION NULL DEFAULT NULL,
	"spd_pred" DOUBLE PRECISION NULL DEFAULT NULL,
	"tech" INTEGER NULL DEFAULT NULL,
	"cap" DOUBLE PRECISION NULL DEFAULT NULL,
	"height" DOUBLE PRECISION NULL DEFAULT NULL,
	"azimuth" DOUBLE PRECISION NULL DEFAULT NULL
)
;

CREATE TABLE "run_test" (
	"model_name" VARCHAR NULL DEFAULT NULL,
	"start" TIMESTAMP NULL DEFAULT NULL,
	"stop" TIMESTAMP NULL DEFAULT NULL,
	"pid" INTEGER NULL DEFAULT NULL
)
;

CREATE TABLE "run_train" (
	"model_name" VARCHAR NULL DEFAULT NULL,
	"start" TIMESTAMP NULL DEFAULT NULL,
	"stop" TIMESTAMP NULL DEFAULT NULL,
	"pid" INTEGER NULL DEFAULT NULL
)
;

CREATE TABLE "test" (
	"id" INTEGER NULL DEFAULT NULL,
	"date_" DATE NULL DEFAULT NULL,
	"x" DOUBLE PRECISION NULL DEFAULT NULL,
	"y" DOUBLE PRECISION NULL DEFAULT NULL,
	"tech" INTEGER NULL DEFAULT NULL,
	"cap" DOUBLE PRECISION NULL DEFAULT NULL,
	"height" DOUBLE PRECISION NULL DEFAULT NULL,
	"azimuth" DOUBLE PRECISION NULL DEFAULT NULL
)
;

CREATE TABLE "train" (
	"id" INTEGER NULL DEFAULT NULL,
	"date_" DATE NULL DEFAULT NULL,
	"x" DOUBLE PRECISION NULL DEFAULT NULL,
	"y" DOUBLE PRECISION NULL DEFAULT NULL,
	"spd" DOUBLE PRECISION NULL DEFAULT NULL,
	"subs" DOUBLE PRECISION NULL DEFAULT NULL,
	"trf" DOUBLE PRECISION NULL DEFAULT NULL,
	"tech" INTEGER NULL DEFAULT NULL,
	"cap" DOUBLE PRECISION NULL DEFAULT NULL,
	"height" DOUBLE PRECISION NULL DEFAULT NULL,
	"azimuth" DOUBLE PRECISION NULL DEFAULT NULL
)
;
